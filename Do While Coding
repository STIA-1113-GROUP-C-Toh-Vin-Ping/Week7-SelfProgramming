package onlineClass;

public class DoWhile {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x =1;
		int y = 10;
		int z =20;
		do {
			System.out.print(x + " ");
			x++;
		}while(x<=10);
		System.out.println();
		
		do {
			System.out.print(y+ " ");
			y=y+1;//same as y++
		}while(y<=30);
		System.out.println();
		
		do {
			System.out.print(z+ " ");
			z--;
	}while(z>=10);
		
	}

	}
