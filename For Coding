package onlineClass;

public class For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		for (int x = 1; x <=10; x++ ) {
			System.out.print(x+ " ");
		}
System.out.println();

		int y;
		for (y = 1; y <=10; y++ ) {
		System.out.print(y+ " ");
		}
		System.out.println();
		
		for (int z = 20; z >=10; z-- ) {
			System.out.print(z+ " ");
		}
		
	}

}
